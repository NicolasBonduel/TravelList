{
  "main": "index.jsx",
  "scripts": {
    "start": "env ENV=qa npx webpack serve --config ./webpack/webpack.dev.config.js",
    "build:prod": "env ENV=production npx webpack --config ./webpack/webpack.prod.config.js",
    "build:qa": "env ENV=qa npx webpack --config ./webpack/webpack.prod.config.js",
    "lint": "npx eslint .",
    "pretty": "npx prettier .",
    "test": "npx jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "npx eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md)": [
      "npx prettier --write"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^hooks(.*)": "<rootDir>/src/hooks$1",
      "^components(.*)": "<rootDir>/src/components$1",
      "^src(.*)": "<rootDir>/src$1",
      "^__mocks__(.*)": "<rootDir>/__mocks__$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss|css)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@storybook/addon-actions": "^6.3.5",
    "@storybook/addon-essentials": "^6.3.1",
    "@storybook/addon-links": "^6.3.1",
    "@storybook/addon-storyshots": "^6.3.1",
    "@storybook/builder-webpack5": "^6.3.1",
    "@storybook/manager-webpack5": "^6.3.1",
    "@storybook/react": "^6.3.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.9.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.2",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.34.1",
    "sass-loader": "^12.1.0",
    "simple-git-hooks": "^2.5.1",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "4.0.0-beta.3",
    "webpack-merge": "^5.8.0"
  }
}
